<body>
    <div id="page-container" class="sidebar-l sidebar-o side-scroll header-navbar-fixed">
        <nav id="sidebar">
            <div></div>
            <div id="sidebar-scroll">
                <div class="sidebar-content">
                    <!-- Side Header -->
                    <div class="side-header side-content bg-white-op">
                        <a class="h5 text-white" href="@Url.Action("Index","Home")">
                            <i class="si si-home text-success"></i> <span class="h4 font-w600 sidebar-mini-hide text-white"></span>
                        </a>
                    </div>
                    <!-- END Side Header -->
                    <!-- Side Content -->
                    <div class="side-content">
                        <ul class="nav-main">
                            <li class="nav-main-heading"><span class="sidebar-mini-hide text-white">Menu</span></li>
                            <li>
                                <a class="nav-submenu" data-toggle="nav-submenu" href="#"><i class="si si-note text-white-op"></i><span class="sidebar-mini-hide text-white-op">Chamados</span></a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("IndexAll","Label")">Criar Chamado</a>
                                    </li>
                                </ul>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("IndexNew","Label")">Pesquisar Chamado</a>
                                    </li>
                                </ul>

                            </li>
                        </ul>
                    </div>
                    <!-- END Side Content -->
                </div>
                <!-- Sidebar Content -->
            </div>
            <!-- END Sidebar Scroll Container -->
        </nav>
